<div>
  <svg
    _ngcontent-eoo-c26=""
    xmlns="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    viewBox="0 0 1920 166"
    fill="currentColor"
    style="color: black;"
    ><defs _ngcontent-eoo-c26=""
      ><clipPath _ngcontent-eoo-c26="" id="clip-path"
        ><rect
          _ngcontent-eoo-c26=""
          id="Rectángulo_636"
          data-name="Rectángulo 636"
          width="1920"
          height="166"
          transform="translate(0 6950)"></rect></clipPath
      ><clipPath _ngcontent-eoo-c26="" id="clip-path-2"
        ><rect
          _ngcontent-eoo-c26=""
          id="Rectángulo_388"
          data-name="Rectángulo 388"
