        _ngcontent-eoo-c26=""
        id="Enmascarar_grupo_10"
        data-name="Enmascarar grupo 10"
        transform="translate(0.492)"
        clip-path="url(#clip-path-2)"
        ><g
          _ngcontent-eoo-c26=""
          id="Grupo_53"
          data-name="Grupo 53"
          transform="translate(-2807.831 6890.59)"
          ><g
            _ngcontent-eoo-c26=""
            id="Grupo_52"
            data-name="Grupo 52"
            transform="translate(0 0)"
            clip-path="url(#clip-path-3)"
            ><path
              _ngcontent-eoo-c26=""
              id="Trazado_60"
              data-name="Trazado 60"
              d="M4927.708,2.894c-307.765,0-307.765,134.891-615.507,134.891S4004.46,2.894,3696.717,2.894c-307.765,0-307.765,134.891-615.507,134.891-307.765,0-307.765-134.891-615.507-134.891S2157.961,137.785,1850.2,137.785c-307.742,0-307.742-134.891-615.484-134.891S926.97,137.785,619.228,137.785C311.463,137.785,311.463,2.894,3.721,2.894v574.72H6158.7V2.894c-307.744,0-307.744,134.891-615.509,134.891-307.741,0-307.741-134.891-615.483-134.891"
